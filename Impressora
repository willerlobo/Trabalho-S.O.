public class Impressora implements Runnable{
     private Armazem num;

    public Impressora(Armazem num) {
        this.num = num;
    }
    
    
    public void run(){
        for(int i = 0; i < 10; i++){
           
         System.out.println("valor" + this.num.getNumero());
        }
    }
}
